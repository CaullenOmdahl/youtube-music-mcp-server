{
  "name": "youtube-music-mcp-server",
  "version": "3.0.0",
  "description": "Enterprise-grade MCP server for YouTube Music with smart playlist creation",
  "main": "dist/index.js",
  "module": "./src/index.ts",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mcp",
    "youtube-music",
    "ai",
    "playlist",
    "musicbrainz",
    "listenbrainz"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "@smithery/sdk": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "googleapis": "^144.0.0",
    "got": "^14.4.2",
    "lru-cache": "^11.2.2",
    "musicbrainz-api": "^0.16.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "tough-cookie": "^4.1.4",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.15.6",
    "@types/tough-cookie": "^4.0.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.47.0"
  }
}
